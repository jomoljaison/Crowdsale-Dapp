    <div class="container">
        <center>
            <p style="font-size:50px;color:rgb(13, 41, 165)">ADMIN PANEL</p>
        </center>

        <% if(action=='add' ) { %>

            <div class="card">
                <div class="card-header">Add Address</div>
                <div class="card-body">
                    <form method="POST" action="/whitelist/add_whitelist">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="mb-3">

                                    <input type="text" name="address" id="address" class="form-control" />
                                </div>
                            </div>

                        </div>

                        <div class="mb-3">
                            <input type="submit" name="submit_button" class="btn btn-primary" value="Add" />
                        </div>
                    </form>
                </div>



            </div>

            <% } else if(action=='edit' ) { %>
                <div class="card">
                    <div class="card-header">Sample Form</div>
                    <div class="card-body">
                        <form method="POST" action="/whitelist/edit/<%= sampleData.id %>">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="mb-3">
                                        <label>Address</label>
                                        <input type="text" name="address" id="address" class="form-control"
                                            value="<%= sampleData.address %>" />
                                    </div>
                                </div>

                            </div>

                            <div class="mb-3">
                                <input type="submit" name="submit_button" class="btn btn-primary" value="Edit" />
                            </div>
                        </form>
                        <script>
                        </script>
                    </div>
                </div>
                <% } else { %>
                    <% if(message.length> 0) { %>
                        <div class="alert alert-success">
                            <%= message %>
                        </div>
                        <% } %>
                            <div class="card">
                                <div class="card-header">
                                    <div class="row">
                                        <div class="col">whitelist</div>
                                        <div class="col">
                                            <a href="/whitelist/add" class="btn btn-success btn-sm float-end">Add</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table table-bordered">
                                            <tr>
                                                <th>Address</th>

                                            </tr>
                                            <% if(sampleData.length> 0)
                                                {
                                                sampleData.forEach(function(data){
                                                %>
                                                <tr>
                                                    <td>
                                                        <%= data.address %>
                                                    </td>
                                                    <td>
                                                        <!-- <a href="/whitelist/edit/<%= data.id %>" class="btn btn-primary btn-sm">Edit</a> -->
                                                        <a href="/whitelist/delete/<%= data.id %>"
                                                            class="btn btn-danger btn-sm">Delete</a>
                                                    </td>

                                                </tr>

                                                <% }); } else { %>
                                                    <tr>
                                                        <td colspan="5">No Data Found</td>
                                                    </tr>
                                                    <% } %>

                                        </table>

                                    </div>

                                </div>

                                <h2> Root Hash </h2>

                                <td>
                                    <%=rootie %>
                                </td>
                                <h2> Proof</h2>

                                <td>
                                    <%=proof %>
                                </td>
                                <h2>Leaf</h2>
                                <td>
                                    <%=leafd %>
                                </td>



                                <br><br>
                                <hr class="new5">
                                <!------------------------------------------------------------ setWhitelistRoot ------------------------------------------------------------->
                                <form action="setWhitelistRoot" method="post">
                                    <h4>Set New Root</h4>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="mb-3">
                                                <input type="text" size="90" name="setWhitelistRoot"
                                                    id="setWhitelistRoot" value=<%=rootie %>
                                                />
                                            </div>
                                        </div>
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </form>
                                <hr class="new5">

                                <!------------------------------------------------------------ getCurrent Time ------------------------------------------------------------->

                                <button class="button" onclick="getCurrentDate()">Get current time</button>

                                <p>
                                    <!-- Current date/time in milliseconds is:  -->
                                    <span style="display:none;" class="output-msecs">
                                </p>
                                <center>
                                    <p>
                                        <!-- Current date/time in seconds is:  -->
                                        <span class="output-secs">
                                    </p>
                                </center>


                                <p>
                                    <!-- Current date/time in words is:  -->
                                    <span style="display:none;" class="output-words">
                                </p>
                                <hr class="new5">
                                <!------------------------------------------------------------ setPreSaleStart ------------------------------------------------------------->

                                <form action="setPreSaleStart" method="post">
                                    <h4>Set Presale time</h4>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="mb-3">
                                                <input type="number" name="setPreSaleStart" id="setPreSaleStart"
                                                    class="form-control" placeholder="Set Presale time" />
                                            </div>
                                        </div>
                                        <button type="submit" class="btn btn-primary">Submit</button>
                                    </div>
                                </form>
                                <hr class="new5">







                            </div>

                            <% } %>

    </div>

    </center>




    <script type="text/javascript">
        function getCurrentDate() {
            dateInMillisecs = Date.now();

            // rounding the value to prevent inconsistencies
            // due to floating points
            dateInSecs = Math.round(dateInMillisecs / 1000);
            dateInWords = new Date(dateInMillisecs);
            document.querySelector('.output-msecs').textContent =
                dateInMillisecs;
            document.querySelector('.output-secs').textContent =
                dateInSecs;
            document.querySelector('.output-words').textContent =
                dateInWords;
        }
    </script>





